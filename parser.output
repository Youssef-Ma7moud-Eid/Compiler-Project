Grammar

    0 $accept: program $end

    1 program: VOID MAIN '(' ')' '{' statements '}'

    2 statements: /* empty */
    3           | statements statement

    4 statement: declaration ';'
    5          | init_declaration ';'
    6          | assignment ';'
    7          | print_stmt ';'
    8          | if_stmt
    9          | expression ';'
   10          | '{' statements '}'

   11 declaration: INT VARIABLE
   12            | FLOAT VARIABLE
   13            | DOUBLE VARIABLE

   14 init_declaration: INT VARIABLE '=' expression
   15                 | FLOAT VARIABLE '=' expression
   16                 | DOUBLE VARIABLE '=' expression

   17 assignment: VARIABLE '=' expression

   18 print_stmt: PRINT '(' expression ')'

   19 if_stmt: simple_if
   20        | if_else
   21        | if_else_if

   22 simple_if: IF '(' expression ')' '{' statements '}'

   23 if_else: IF '(' expression ')' '{' statements '}' ELSE '{' statements '}'

   24 if_else_if: IF '(' expression ')' '{' statements '}' ELSE if_stmt

   25 expression: NUMBER
   26           | FLOAT_NUM
   27           | DOUBLE_NUM
   28           | VARIABLE
   29           | '!' expression
   30           | '-' expression
   31           | expression '+' expression
   32           | expression '-' expression
   33           | expression '*' expression
   34           | expression '/' expression
   35           | expression GT expression
   36           | expression LT expression
   37           | expression EQ expression
   38           | expression OR expression
   39           | '(' expression ')'


